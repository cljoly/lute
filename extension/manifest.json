{
  "manifest_version": 3,
  "name": "Lute V4",
  "version": "0.0.1",
  "background": {
    "service_worker": "src/background.ts",
    "type": "module"
  },
  "action": {
    "default_popup": "src/index.html"
  },
  "permissions": ["pageCapture", "storage", "tabs", "scripting"],
  "host_permissions": ["http://*/*", "https://*/*"]
}
